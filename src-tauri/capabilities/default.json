{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:app:allow-set-app-theme",
    "dialog:allow-ask",
    "dialog:allow-confirm",
    "dialog:allow-message",
    "dialog:allow-open"
  ]
}
